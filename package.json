  "devDependencies": {
    "@posthog/plugin-scaffold": "^1.4.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/jest": "^29.2.6",
    "bson-objectid": "^2.0.4",
    "husky": "^8.0.3",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
