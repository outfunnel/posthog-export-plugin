{
  "name": "@outfunnel/posthog-export-plugin",
  "version": "0.1.0",
  "description": "Stream event data from PostHog to Outfunnel",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist/*",
    "compile": "rollup -c --bundleConfigAsCjs",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "plugin.json",
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/outfunnel/outfunnel-posthog-export-plugin.git"
  },
  "author": "Outfunnel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/outfunnel/outfunnel-posthog-export-plugin/issues"
  },
  "homepage": "https://github.com/outfunnel/outfunnel-posthog-export-plugin#readme",
  "devDependencies": {
    "@posthog/plugin-scaffold": "^1.4.1",
    "@types/jest": "^29.2.6",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.1",
    "rollup": "^3.10.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "$(npm bin)/lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write"
  }
}
